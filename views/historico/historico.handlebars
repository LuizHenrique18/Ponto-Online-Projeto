<div class="page-historico">
    <div class="item-all-page-historico">
            <div class="perfil-info-historico">
                <div><h2>{{userName}}</h2></div>
                <div><p>{{userEmail}}</p><p>{{cpfFormatado}}</p> </div>
            </div>

        <div class="item-all-page-historico">  
            <div class="container-dados-userss">
                <div class="users-controle">  
                        <div class="div-layout-dados">   
                            <div class="titulo-dados-user"><h2>Data</h2></div>
                            <div class="titulo-dados-user tit-email"><h2>Entrada</h3></div>
                            <div class="titulo-dados-user"><h2>Saida</h2></div>
                            <div class="titulo-dados-user"><h2>Tempo</h2></div>
                            <div class="titulo-dados-user ver-mais"><h2>Mais</h2></div>
                        </div>
                    {{#each dadosPessoa}}
                            <div class="div-layout-dados">   
                                <div class="name-controler"><p translate="no">{{this.datas}}</p></div>
                                <div class="name-controler tit-email"><p translate="no">{{this.entrada}}</p></div>
                                <div class="name-controler"><p translate="no">{{this.saida}}</p></div>
                                <div class="name-controler"><p translate="no">{{this.tempo}}</p></div>
                                <div class="name-controler ver-mais"><a href="/historicoDia{{this.arrayIdData}}" class="historico-a"><img class="img-icon" src="img/lupa.png"></a></div>
                            </div>
                    {{/each}}
                </div>
            </div>

        </div>
    </div>
    
</div>


</div>


</div>