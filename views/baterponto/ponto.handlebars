<div class="all-page" id="page-pontos">

    <div class="bater-ponto">

        <div class="div-user-horarios">
            <div>
                <div id="img-user"></div>
                <p>{{nome}}</p>
                <p>{{email}}</p>
            </div>
            <div>
                <a href="/"><h2 id="horarios-agendamento">Seus Horários</h2></a>
            </div>
        </div> 
            <div class="div-ponto-horarios">
                    
                
                    <div><input type="text" name="timeData" class="timeData" value="00:00:00"></div>
                    <div class="div-ponto-entrada">
                        {{#if butEntrada}}
                        <div class="button-ent" id="item-entrada"><h2>Entrada</h2></div>
                        {{else}}
                        <div class="button-ent" id="item-saida"><h2>Saída</h2></div>
                        {{/if}}
                    </div>  
            </div>

    </div>
        <div class="div-video">
            <div class="div-retrato-video">
                <div class="titulo-retrato-video"><p>TIRAR FOTO PARA REGISTRO DE PONTO</p></div>
                <video id="tirar-foto" autoplay></video>
                <canvas id="canvas"></canvas>
            </div>
            <div class="but-fotos">
                <button id="pararCamera" value="Enviar" class="butfotosenviar">Parar</button>
                <button id="cliqueaqui" value="clique aqui" class="butfotosenviar">Tirar Foto</button>
                <button id="novafoto" value="clique aqui" class="butfotosenviar">Nova foto</button>
                <button id="enviar" value="clique aqui" class="butfotosenviar">Enviar</button>
            </div>
            
        </div>
        
                
                
</div>
<div class="loader"></div>
<script src="js/settings.js"></script>